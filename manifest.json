{
  "manifest_version": 2,

  "name": "Tiktok Profile Analyzer",
  "description": "This extension will analyze a Tiktok profile using API",
  "version": "1.0.1",
  "author": "Maksim Zakharov",
  "icons": {"128": "icon_128x128.png"},
  "content_scripts": [
    {
      "matches": ["*://*.tiktok.com/*"],
      "js": ["popup.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "*://*.tiktok.com/*"
  ]
}
